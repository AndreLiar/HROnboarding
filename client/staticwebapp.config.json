{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://hr-onboarding-api.azurewebsites.net/*"
    },
    {
      "route": "/c/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.{css,scss,js,png,gif,ico,jpg,svg}"]
  }
}